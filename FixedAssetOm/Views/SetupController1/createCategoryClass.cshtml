@model FixedAssetOm.Models.TblFacategory

@{
    ViewData["Title"] = "createCategoryClass";
}

<h1> Create CategoryClass</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="createCategoryClass" method="post" >
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CatCode" class="control-label"></label>
                <input asp-for="CatCode" class="form-control" />
                <span asp-validation-for="CatCode" class="text-danger"></span>
            </div>
            <br />

            <div class="form-group">
                <label asp-for="CatDesc" class="control-label">Category Name</label>
                <input asp-for="CatDesc" class="form-control" />
                <span asp-validation-for="CatDesc" class="text-danger"></span>
            </div>
         <br />
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a href="@Url.Action("")" class="btn btn-primary" id="button">Return</a>
            </div>
        </form>
    </div>
</div>
<br />


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
